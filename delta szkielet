int main()
{
    int a,b,c,del;
    cout << "ale rozjebalem" << endl;
    cout << "Podaj a" << endl;
    cin>>a;
    cout << "Podaj b" << endl;
    cin>>b;
    cout << "Podaj c" << endl;
